{
    "name": "uni-validator",
    "version": "1.0.0",
    "author": "n11",
    "license": "MIT",
    "description": "strong js validator",
    "scripts": {
        "start": "babel -d ./dist ./src -s && jsdoc ./src",
        "watch": "babel --watch -d ./dist ./src -s",
        "lint": "eslint .",
        "lint-staged": "lint-staged",
        "build": "babel src --out-dir distribution",
        "prepublish": "npm run build"
    },
    "keywords": [
        "uni-validator",
        "uni validator",
        "validator",
        "validator js",
        "js validator",
        "validation",
        "strong validation",
        "js validation",
        "custom validation",
        "reg validation",
        "regexp validation",
        "nodejs validation",
        "node validation",
        "best validator"
    ],
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/N1gma/uni-validator.git"
    },
    "homepage": "https://github.com/N1gma/uni-validator",
    "bugs": {
        "url": "https://github.com/N1gma/uni-validator/issues"
    },
    "devDependencies": {
        "babel-cli": "^6.24.1",
        "babel-core": "^6.22.1",
        "babel-eslint": "^7.2.3",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-es2016": "^6.22.0",
        "babel-preset-stage-0": "^6.22.0",
        "eslint": "^3.17.1",
        "eslint-config-airbnb-base": "^11.1.1",
        "eslint-config-prettier": "^1.5.0",
        "eslint-plugin-import": "^2.2.0",
        "jsdoc": "^3.4.3"
    },
    "pre-commit": "lint-staged",
    "lint-staged": {
        "*.js": "lint:js"
    }
}
